": "", "ability": "", "attackBonus": 0, "critical": null, "formula": "", "chatFlavor": "", "damage": {"parts": [], "versatile": ""}, "save": {"ability": "", "dc": null, "scaling": "spell"}, "requirements": "", "recharge": {"value": 0, "charged": false}}, "sort": 30000, "_id": "OTMwYjAxOTVhYjg0"}, {"name": "Bite", "flags": {}, "type": "weapon", "img": "modules/mod-rise-of-tiamat/characters/002_-_Adult_Green_Dragon/avatar.png", "data": {"description": {"value": "<p><p><strong>Bite.</strong><em>Melee Weapon Attack: </em>+11, Reach 10 ft.,  one target. <em>Hit : </em>17 (2d10 + 6) piercing damage plus 7 (2d6) poison damage</p></p>", "chat": "", "unidentified": ""}, "source": "", "activation": {"type": "action", "cost": 1, "condition": ""}, "target": {"value": "", "units": "", "type": ""}, "range": {"value": 10, "long": "", "units": "ft"}, "duration": {"value": 0, "units": ""}, "uses": {"value": 0, "max": 0, "per": ""}, "actionType": "mwak", "ability": "str", "attackBonus": 0, "critical": null, "formula": "", "chatFlavor": "", "damage": {"parts": [["2d10 + 6", "piercing"], ["2d6", "poison"]], "versatile": ""}, "save": {"ability": "", "dc": null, "scaling": "spell"}, "rarity": "", "quantity": 1, "weight": 1, "price": 0, "attuned": false, "equipped": true, "identified": true, "weaponType": "natural", "proficient": true, "properties": {"amm": false, "hvy": false, "fin": false, "fir": false, "foc": false, "lgt": false, "rch": false, "rel": false, "ret": false, "spc": false, "thr": false, "two": false, "ver": false}}, "sort": 40000, "_id": "MDNjNmZkYzI5ZDU1"}, {"name": "Claw", "flags": {}, "type": "weapon", "img": "modules/mod-rise-of-tiamat/characters/002_-_Adult_Green_Dragon/avatar.png", "data": {"description": {"value": "<p><p><strong>Claw.</strong><em>Melee Weapon Attack: </em>+11, Reach 5 ft.,  one target. <em>Hit : </em>13 (2d6 + 6) slashing damage</p></p>", "chat": "", "unidentified": ""}, "source": "", "activation": {"type": "action", "cost": 1, "condition": ""}, "tar