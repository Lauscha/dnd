{"_id": "YzgwMGY2Yjg3MDNl", "name": "Tentacles (Chuul)", "permission": {"default": 0}, "folder": null, "flags": {}, "type": "feat", "img": "modules/mod-rise-of-tiamat/characters/012_-_Chuul/avatar.png", "data": {"description": {"value": "<p><p><strong>Tentacles.</strong>.</p><p>One creature grappled by the chuul must succeed on a DC 13 Constitution saving throw or be poisoned for 1 minute. Until this poison ends, the target is paralyzed. The target can repeat the saving throw at the end of each of its turns, ending the effect on itself on a success.</p><p>  </p></p>", "chat": "", "unidentified": ""}, "source": "", "activation": {"type": "action", "cost": 1, "condition": ""}, "target": {"value": "", "units": "", "type": ""}, "range": {"value": "", "long": "", "units": ""}, "duration": {"value": 0, "units": ""}, "uses": {"value": 0, "max": 0, "per": ""}, "actionType": "", "ability": "", "attackBonus": 0, "critical": null, "formula": "", "chatFlavor": "", "damage": {"parts": [], "versatile": ""}, "save": {"ability": "con", "dc": 13, "scaling": "spell"}, "requirements": "", "recharge": {"value": 0, "charged": false}}, "sort": 910000}
{"_id": "MDc1NWRhN2I4Y2Rj", "name": "Acid Absorption. (Clay Golem)", "permission": {"default": 0}, "folder": null, "flags": {}, "type": "feat", "img": "modules/mod-rise-of-tiamat/characters/013_-_Clay_Golem/avatar.png", "data": {"description": {"value": "<p>Whenever the golem is subjected to acid damage, it takes no damage and instead regains a number of hit points equal to the acid damage dealt.</p>", "chat": "", "unidentified": ""}, "source": "", "activation": {"type": "", "cost": 0, "condition": ""}, "target": {"value": "", "units": "", "type": ""}, "range": {"value": "", "long": "", "units": ""}, "duration": {"value": 0, "units": ""}, "uses": {"value": 0, "max": 0, "per": ""}, "actionType": "", "ability": "", "attackBonus": 0, "critical": null, "formula": "", "chatFlavor": "", "damage": {"parts": [], "versatile": ""}, "save": {"ability": "", "dc": null, "scaling": "spell"}, "requirements": "", "recharge": {"value": 0, "charged": false}}, "sort": 920000}
