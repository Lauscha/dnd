{"_id": "ZDQzMTQ4YWQzOWJh", "name": "Limited Telepathy. (Otyugh)", "permission": {"default": 0}, "folder": null, "flags": {}, "type": "feat", "img": "modules/mod-rise-of-tiamat/characters/043_-_Otyugh/avatar.png", "data": {"description": {"value": "<p>The otyugh can magically transmit simple messages and images to any creature within 120 ft. of it that can understand a language. This form of telepathy doesn't allow the receiving creature to telepathically respond.</p>", "chat": "", "unidentified": ""}, "source": "", "activation": {"type": "", "cost": 0, "condition": ""}, "target": {"value": "", "units": "", "type": ""}, "range": {"value": "", "long": "", "units": ""}, "duration": {"value": 0, "units": ""}, "uses": {"value": 0, "max": 0, "per": ""}, "actionType": "", "ability": "", "attackBonus": 0, "critical": null, "formula": "", "chatFlavor": "", "damage": {"parts": [], "versatile": ""}, "save": {"ability": "", "dc": null, "scaling": "spell"}, "requirements": "", "recharge": {"value": 0, "charged": false}}, "sort": 3260000}
{"_id": "Mjk5NGY0OTI1NWU4", "name": "Multiattack (Otyugh)", "permission": {"default": 0}, "folder": null, "flags": {}, "type": "feat", "img": "modules/mod-rise-of-tiamat/characters/043_-_Otyugh/avatar.png", "data": {"description": {"value": "<p><p><strong>Multiattack.</strong>.</p><p>The otyugh makes three attacks: one with its bite and two with its tentacles.</p><p></p><p></p><p></p><p></p></p>", "chat": "", "unidentified": ""}, "source": "", "activation": {"type": "action", "cost": 1, "condition": ""}, "target": {"value": "", "units": "", "type": ""}, "range": {"value": "", "long": "", "units": ""}, "duration": {"value": 0, "units": ""}, "uses": {"value": 0, "max": 0, "per": ""}, "actionType": "", "ability": "", "attackBonus": 0, "critical": null, "formula": "", "chatFlavor": "", "damage": {"parts": [], "versatile": ""}, "save": {"ability": "", "dc": null, "scaling": "spell"}, "requirements": "", "recharge": {"value": 0, "charged": false}}, "sort": 3270000}
{"_id": "MjdlYTc4MzVlZDA0", "name": "Bite (Otyugh)", "permission": {"default": 0}, "folder": null, "flags": {}, "type": "weapon", "img": "modules/mod-rise-of-tiamat/characters/043_-_Otyugh/avatar.png", "data": {"description": {"value": "<p><p><strong>Bite.</strong><em>Melee Weapon Attack: </em>+6, Reach 5 ft.,  one target. <em>Hit : </em>12 (2d8 + 3) piercing damage.</p><p>If the target is a creature, it must succeed on a DC 15 Constitution saving throw against disease or become poisoned until the disease is cured. Every 24 hours that elapse, the target must repeat the saving throw, reducing its hit point maximum by 5, 1d10, on a failure. The disease is cured on a success. The target dies if the disease reduces its hit point maximum to 0. This reduction to the target's hit point maximum lasts until the disease is cured.</p></p>", "chat": "", "unidentified": ""}, "source": "", "activation": {"type": "action", "cost": 1, "condition": ""}, "target": {"value": "", "units": "", "type": ""}, "range": {"value": 5, "long": "", "units": "ft"}, "duration": {"value": 0, "units": ""}, "uses": {"value": 0, "max": 0, "per": ""}, "actionType": "mwak", "ability": "str", "attackBonus": 0, "critical": null, "formula": "", "chatFlavor": "", "damage": {"parts": [["2d8 + 3", "piercing"]], "versatile": ""}, "save": {"ability": "con", "dc": 15, "scaling": "spell"}, "rarity": "", "quantity": 1, "weight": 1, "price": 0, "attuned": false, "equipped": true, "identified": true, "weaponType": "natural", "proficient": true, "properties": {"amm": false, "hvy": false, "fin": false, "fir": false, "foc": false, "lgt": false, "rch": false, "rel": false, "ret": false, "spc": false, "thr": false, "two": false, "ver": false}}, "sort": 3280000}
