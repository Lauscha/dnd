{"_id": "M2EzODBhMDRjNzZj", "name": "Multiattack (Wight)", "permission": {"default": 0}, "folder": null, "flags": {}, "type": "feat", "img": "modules/mod-rise-of-tiamat/characters/057_-_Wight/avatar.png", "data": {"description": {"value": "<p><p><strong>Multiattack.</strong>.</p><p>The wight makes two longsword attacks or two longbow attacks. It can use its Life Drain in place of one longsword attack.</p><p></p><p></p><p></p><p></p></p>", "chat": "", "unidentified": ""}, "source": "", "activation": {"type": "action", "cost": 1, "condition": ""}, "target": {"value": "", "units": "", "type": ""}, "range": {"value": "", "long": "", "units": ""}, "duration": {"value": 0, "units": ""}, "uses": {"value": 0, "max": 0, "per": ""}, "actionType": "", "ability": "", "attackBonus": 0, "critical": null, "formula": "", "chatFlavor": "", "damage": {"parts": [], "versatile": ""}, "save": {"ability": "", "dc": null, "scaling": "spell"}, "requirements": "", "recharge": {"value": 0, "charged": false}}, "sort": 3930000}
