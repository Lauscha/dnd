{"_id": "YWFiYzgxY2I2ODNm", "name": "Earth Glide. (Earth Elemental)", "permission": {"default": 0}, "folder": null, "flags": {}, "type": "feat", "img": "modules/mod-rise-of-tiamat/characters/020_-_Earth_Elemental/avatar.png", "data": {"description": {"value": "<p>The elemental can burrow through nonmagical, unworked earth and stone. While doing so, the elemental doesn't disturb the material it moves through.</p>", "chat": "", "unidentified": ""}, "source": "", "activation": {"type": "", "cost": 0, "condition": ""}, "target": {"value": "", "units": "", "type": ""}, "range": {"value": "", "long": "", "units": ""}, "duration": {"value": 0, "units": ""}, "uses": {"value": 0, "max": 0, "per": ""}, "actionType": "", "ability": "", "attackBonus": 0, "critical": null, "formula": "", "chatFlavor": "", "damage": {"parts": [], "versatile": ""}, "save": {"ability": "", "dc": null, "scaling": "spell"}, "requirements": "", "recharge": {"value": 0, "charged": false}}, "sort": 1530000}
{"_id": "MDViNDBlMzIwNWMy", "name": "Siege Monster. (Earth Elemental)", "permission": {"default": 0}, "folder": null, "flags": {}, "type": "feat", "img": "modules/mod-rise-of-tiamat/characters/020_-_Earth_Elemental/avatar.png", "data": {"description": {"value": "<p>The elemental deals double damage to objects and structures.</p>", "chat": "", "unidentified": ""}, "source": "", "activation": {"type": "", "cost": 0, "condition": ""}, "target": {"value": "", "units": "", "type": ""}, "range": {"value": "", "long": "", "units": ""}, "duration": {"value": 0, "units": ""}, "uses": {"value": 0, "max": 0, "per": ""}, "actionType": "", "ability": "", "attackBonus": 0, "critical": null, "formula": "", "chatFlavor": "", "damage": {"parts": [], "versatile": ""}, "save": {"ability": "", "dc": null, "scaling": "spell"}, "requirements": "", "recharge": {"value": 0, "charged": false}}, "sort": 1540000}
